<?xml version="1.0"?>
<package >
  <metadata>
    <id>Tuohy.Epi.ContentTypesInUse</id>
    <version>1.0.0.0</version>
    <title>EPiServer Content Type Usage Information</title>
    <authors>Dan Tuohy</authors>
    <owners>Dan Tuohy</owners>
    <projectUrl>http://dantuohy.me/blog-posts/episerver-content-types-in-use-addon/</projectUrl>
